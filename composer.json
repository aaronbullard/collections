{
    "name": "aaronbullard/collections",
    "description": "Generic class to make a collection of any object type.",
    "authors": [
        {
            "name": "aaronbullard",
            "email": "JABullard@aol.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
		"mockery/mockery": "0.9.*",
		"phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Aaronbullard\\Collections\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --colors=always --coverage-html=./coverage --coverage-text=./tests/coverage.txt"
        ]
    },
    "minimum-stability": "stable"
}
